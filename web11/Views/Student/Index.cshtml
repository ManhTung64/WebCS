@model List<Student>

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>DoB</th>
                <th>Email</th>
                <th>Grade</th>
                <th>Image</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var student in Model)
            {
                <tr>
                    <td>@student.StudentId</td>
                    <td>@student.Name</td>
                    <td>@student.DoB.ToLongDateString()</td>
                    @*.Year to get year, .Now.Year to get now year*@
                    <td>@student.Email</td>
                    <td>@student.Grade</td>
                    <td><img src="@student.Image" width="100" height="100"/></td>
                </tr>
            }
        </tbody>
   </table>