@model web11.Models.Student

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>DoB</th>
                <th>Email</th>
                <th>Grade</th>
                <th>Image</th>
            </tr>
        </thead>
        <tbody>
                    <tr>
                        <td>@Model.StudentId</td>
                        <td>@Model.Name</td>
                        <td>@Model.DoB.ToLongDateString()</td>
                        @*.Year to get year, .Now.Year to get now year*@
                        <td>@Model.Email</td>
                        <td>@Model.Grade</td>
                        <td><img src="@Model.Image" width="100" height="100"/></td>
                    </tr>
        </tbody>
   </table>